<div class="page-header">
  <%= link_to certtests_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_certtest_path(@certtest), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show certtest</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @certtest.name %></dd>

  <dt>Description:</dt>
  <dd><%= @certtest.description %></dd>

  <dt>Project:</dt>
  <dd><%= @certtest.project %></dd>

</dl>
